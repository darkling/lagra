	<#ruby-rdf-nomatch-test3>
	<#ruby-rdf-nomatch-test4>
	<#ruby-rdf-nomatch-test5>
	<#ruby-rdf-nomatch-test6>
    ).

<#jena-iso_00>
   mf:action "jena-iso_00-a.nt";
   mf:action "jena-iso_00-b.nt";
   mf:name "jena-iso_00";
   a rdft:TestIsomorphismPositive;
   rdfs:comment "Comparing empty graphs".

