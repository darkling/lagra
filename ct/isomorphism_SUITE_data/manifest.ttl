	<#jena-iso_21>
	<#jena-iso_22>
	<#jena-iso_23>
	<#jena-iso_24>
	# jena-iso_50 to jena_iso_51 omitted: we don't deal with separate graphs
	# jena-iso_61 omitted: identical to jena-iso_00
	<#jena-iso_62>
	<#jena-iso_63>
